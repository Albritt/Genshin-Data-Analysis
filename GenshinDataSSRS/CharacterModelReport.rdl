  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GenshinDataDB</DataSourceName>
        <CommandText>SELECT
	GCIT.model,
	SUM(GRT.avg_revenue)/
	(CASE 
		WHEN MAX(GRT.rerun) = 0 THEN 1
		ELSE MAX(GRT.rerun) + 1 
		END) as total_revenue
	FROM dbo.GenshinRevenueTable as GRT
	LEFT JOIN dbo.GenshinCharacterInfoTable as GCIT on GRT."5_star_characters" = GCIT.character_name
	WHERE GCIT.playable = 'Y'
	GROUP BY GCIT.model</CommandText>
      </Query>
      <Fields>
        <Field Name="model">
          <DataField>model</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="total_revenue">
          <DataField>total_revenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
